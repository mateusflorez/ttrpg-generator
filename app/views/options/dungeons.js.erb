$(function(){

    //Set dice
    const dices = [20, 10, 100, 100, 20, 20, 100, 20];

    //Randomize
    <%= render :partial => 'options/shared/randomizer' %>

    //Area randomizers
    <%= render :partial => 'options/dungeons/area/passage' %>
    <%= render :partial => 'options/dungeons/area/door' %>
    <%= render :partial => 'options/dungeons/area/stair' %>
    <%= render :partial => 'options/dungeons/area/room' %>
    <%= render :partial => 'options/dungeons/area/architecture' %>
    <%= render :partial => 'options/dungeons/area/secretdoor' %>
    <%= render :partial => 'options/dungeons/area/trap' %>
    <%= render :partial => 'options/dungeons/area/clue' %>

    //Option selector
    var res;
    switch (<%= @resp %>) {
        //Mission
        case 1:
            <%= render :partial => 'options/dungeons/dungeon' %>
            break;
        //Source
        case 2:
            <%= render :partial => 'options/dungeons/area' %>
            break;
    }

    //Set history
    <%= render :partial => 'options/shared/set_history' %>

    //Save
    <%= render :partial => 'options/shared/save' %>

    //Print
    <%= render :partial => 'options/shared/print' %>

});
