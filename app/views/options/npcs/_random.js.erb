//Race
res += raceRandomizer();

//Gender
res += genderRandomizer();

//Alignment
res += alignmentRandomizer();

//Profession
rand = minMaxRandomizer(1, 100);
if (rand < 26) {
    res += "Plebeu (desempregado).";
} else if (rand > 25 && rand < 91) {
    res += professionRandomizer();
} else {
    res += adventurerClassRandomizer();
}

//Disposition
res += dispositionRandomizer();

//Economic status
res += economicStatusRandomizer();
