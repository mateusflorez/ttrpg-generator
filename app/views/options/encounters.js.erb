$(function(){

    //Set dice
    const dices = [100, 100, 100, 100, 100];

    //Randomize
    <%= render :partial => 'options/shared/randomizer' %>

    //Option selector
    var res;
    switch (<%= @resp %>) {
        //Wild
        case 1:
            <%= render :partial => 'options/encounters/wild' %>
            break;
        //Urban
        case 2:
            <%= render :partial => 'options/encounters/urban' %>
            break;
        //Dungeon
        case 3:
            <%= render :partial => 'options/encounters/dungeon' %>
            break;
    }

    //Set history
    <%= render :partial => 'options/shared/set_history' %>

    //Save
    <%= render :partial => 'options/shared/save' %>

    //Print
    <%= render :partial => 'options/shared/print' %>

});
