function passageRandomizer() {
    var passage = "";
    if (rndInt[1] === 1) {
        passage = "Passagem. A passagem continua "+minMaxRandomizer(1, 4)*10+" pés.";
    } else if (rndInt[1] === 2) {
        passage = "Passagem. A passagem vai 15 pés e termina em uma porta. "+roomRandomizer();
    } else if (rndInt[1] === 3) {
        passage = "Passagem. A passagem vai 30 pés e termina em escadas. "+stairRandomizer();
    } else if (rndInt[1] === 4) {
        passage = "Passagem. Passagem vira à esquerda 90 graus.";
    } else if (rndInt[1] === 5) {
        passage = "Passagem. Passagem vira à direita 90 graus.";
    } else if (rndInt[1] === 6) {
        passage = "Passagem sem saída. Retorne ao último recurso que deseja explorar.";
    } else if (rndInt[1] === 7) {
        passage = "Passagem. A passagem continua "+minMaxRandomizer(1, 4)*10+" pés e chega a uma interseção de quatro vias.";
    } else if (rndInt[1] === 8) {
        passage = "Passagem. A passagem continua "+minMaxRandomizer(1, 4)*10+" pés e chega a um entroncamento.";
    } else if (rndInt[1] === 9) {
        passage = "Passagem. A passagem continua "+minMaxRandomizer(1, 6)*10+" pés e então você vê uma passagem lateral que leva à esquerda.";
    } else if (rndInt[1] === 10) {
        passage = "Passagem. A passagem continua "+minMaxRandomizer(1, 6)*10+" pés e então você vê uma passagem lateral que leva à direita.";
    } else if (rndInt[1] === 11) {
        passage = "Passagem. A passagem termina em uma entrada aberta para uma sala. "+roomRandomizer();
    } else if (rndInt[1] === 12) {
        passage = "Passagem. Porta na parede direita. "+doorRandomizer();
    } else if (rndInt[1] === 13) {
        passage = "Passagem. Porta na parede esquerda. "+doorRandomizer();
    } else if (rndInt[1] === 14) {
        passage = "Passagem. Porta secreta na parede da passagem (escolha do jogador qual parede). Role percepção CD 15. Se for bem sucedido, vá para a Porta Secreta ???????. Caso contrário, role novamente, continuando (já que seu personagem não percebeu).";
    } else if (rndInt[1] === 15) {
        passage = "Passagem. Passagem estreita "+(minMaxRandomizer(1, 6)/2)*10+" pés";
    } else if (rndInt[1] === 16) {
        passage = "Passagem. Passagem larga "+(minMaxRandomizer(1, 6)/2)*10+" pés";
    } else if (rndInt[1] === 17) {
        passage = "Passagem. Abertura à esquerda, escada. "+stairRandomizer();
    } else if (rndInt[1] === 18) {
        passage = "Passagem. Abertura à direita, escada. "+stairRandomizer();
    } else if (rndInt[1] === 19) {
        temp1 = minMaxRandomizer(1, 100);
        var temp = "";
        if (temp1 < 51) {
            temp += "passagem. "+passageRandomizer();
        } else {
            temp += "sala. "+roomRandomizer();
        }
        passage = "Passagem. Abertura no chão, queda reta "+minMaxRandomizer(1, 10)*10+" pés para uma "+temp;
    } else {
        passage = "Passagem. Rolar na tabela de arquitetura aleatória. ???????.";
    }

    //part 2
    if (rndInt[3] < 70) {
        passage += " A passagem está vazia.";
    } else if (rndInt[3] > 69 && rndInt[3] < 81) {
        temp1 = minMaxRandomizer(1, 100);
        var temp2 = "";
        if (temp1 < 11) {
            temp2 = " Pista! Teste de percepção CD 10 para encontrar.";
        }
        passage += " A passagem está vazia além do entulho."+temp2;
    } else if (rndInt[3] > 80 && rndInt[3] < 85) {
        temp1 = minMaxRandomizer(1, 100);
        var temp2 = "";
        if (temp1 < 21) {
            temp2 = " Pista no corpo!";
        }
        passage += " A passagem está vazia além de um cadáver."+temp2;
    } else if (rndInt[3] > 84 && rndInt[3] < 89) {
        temp1 = minMaxRandomizer(1, 100);
        var temp2 = "";
        if (temp1 < 41) {
            temp2 = " Pista nos corpos!";
        }
        passage += " A passagem está vazia além de vários cadáveres."+temp2;
    } else if (rndInt[3] > 88 && rndInt[3] < 91) {
        temp1 = minMaxRandomizer(1, 100);
        var temp2 = "";
        var temp3 = "";
        if (temp1 < 16) {
            temp2 = " Role saque!";
        }
        temp1 = minMaxRandomizer(1, 100);
        if (temp1 < 16) {
            temp3 = " Role pista!";
        }
        passage += " A passagem está vazia além dos inimigos. Encontro fácil apropriado ao nível."+temp2+temp3;
    } else if (rndInt[3] > 90 && rndInt[3] < 93) {
        temp1 = minMaxRandomizer(1, 100);
        var temp2 = "";
        var temp3 = "";
        if (temp1 < 26) {
            temp2 = " Role saque!";
        }
        temp1 = minMaxRandomizer(1, 100);
        if (temp1 < 26) {
            temp3 = " Role pista!";
        }
        passage += " A passagem está vazia além dos inimigos. Encontro médio apropriado ao nível."+temp2+temp3;
    } else if (rndInt[3] > 92 && rndInt[3] < 95) {
        temp1 = minMaxRandomizer(1, 100);
        var temp2 = "";
        var temp3 = "";
        if (temp1 < 51) {
            temp2 = " Role saque!";
        }
        temp1 = minMaxRandomizer(1, 100);
        if (temp1 < 51) {
            temp3 = " Role pista!";
        }
        passage += " A passagem está vazia além dos inimigos. Encontro difícil apropriado ao nível."+temp2+temp3;
    } else if (rndInt[3] > 94 && rndInt[3] < 99) {
        passage += " Armadilha! ???????.";
    } else {
        temp1 = minMaxRandomizer(1, 100);
        if (temp1 < 61) {
            passage += " Saque!";
        } else {
            passage += " A passagem está vazia.";
        }
    }
    return passage;
}
