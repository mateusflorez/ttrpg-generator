function trapRandomizer() {
    rand = minMaxRandomizer(1, 100);
    var trap = "";
    if (rand < 6) {
        trap = "Armadilha: Dardos envenenados.";
    } else if (rand > 6 && rand < 13) {
        trap = "Armadilha: Telhado desmoronando.";
    } else if (rand > 12 && rand < 20) {
        trap = "Armadilha: Poço Simples.";
    } else if (rand > 19 && rand < 27) {
        trap = "Armadilha: Poço escondido.";
    } else if (rand > 26 && rand < 33) {
        trap = "Armadilha: Fossa de bloqueio.";
    } else if (rand > 32 && rand < 39) {
        trap = "Armadilha: Poço cravado.";
    } else if (rand > 38 && rand < 45) {
        trap = "Armadilha: Esfera rolante.";
    } else if (rand > 44 && rand < 51) {
        trap = "Armadilha: Lâmina de foice.";
    } else if (rand > 50 && rand < 57) {
        temp1 = minMaxRandomizer(1, 4);
        var temp = "";
        if (temp1 === 1) {
            temp += "fogo.";
        } else if (temp1 === 2) {
            temp += "gelo.";
        } else if (temp1 === 3) {
            temp += "ácido.";
        } else {
            temp += "relâmpago.";
        }
        trap = "Armadilha: Armadilha de glifo feitiço de "+temp;
    } else if (rand > 56 && rand < 64) {
        trap = "Armadilha: Feitiço de mísseis mágicos.";
    } else if (rand > 63 && rand < 70) {
        trap = "Armadilha: Gás venenoso liberado.";
    } else if (rand > 69 && rand < 77) {
        trap = "Armadilha: O quarto se enche de água.";
    } else if (rand > 76 && rand < 83) {
        trap = "Armadilha: Paredes começam a fechar.";
    } else if (rand > 82 && rand < 89) {
        trap = "Armadilha: Lanças saem do chão.";
    } else if (rand > 88 && rand < 94) {
        trap = "Armadilha: Gotas de magma.";
    } else {
        trap = "Armadilha: Alçapão (cobras / ácido abaixo?).";
    }

    rand = minMaxRandomizer(1, 100);
    if (rand < 6 && rand < 13) {
        trap += " CD: 10.";
    } else if (rand > 12 && rand < 20) {
        trap += " CD: 11.";
    } else if (rand > 19 && rand < 33) {
        trap += " CD: 12.";
    } else if (rand > 32 && rand < 39) {
        trap += " CD: 13.";
    } else if (rand > 38 && rand < 51) {
        trap += " CD: 14.";
    } else if (rand > 50 && rand < 64) {
        trap += " CD: 15";
    } else if (rand > 63 && rand < 77) {
        trap += " CD: 16";
    } else if (rand > 76 && rand < 83) {
        trap += " CD: 17";
    } else if (rand > 82 && rand < 89) {
        trap += " CD: 18";
    } else if (rand > 88 && rand < 94) {
        trap += " CD: 19";
    } else {
        trap += " CD: 20";
    }

    rand = minMaxRandomizer(1, 100);
    rand1 = minMaxRandomizer(1, 6);
    if (rand < 6) {
        trap += " Dano: "+rand1+" X Nível do personagem -3 (mínimo 1)";
    } else if (rand > 6 && rand < 13) {
        trap += " Dano: "+rand1+" X Nível do personagem -2 (mínimo 1)";
    } else if (rand > 12 && rand < 27) {
        trap += " Dano: "+rand1+" X Nível do personagem -1 (mínimo 1)";
    } else if (rand > 26 && rand < 45) {
        trap += " Dano: "+rand1+" X Nível do personagem";
    } else if (rand > 44 && rand < 70) {
        trap += " Dano: "+rand1+" X Nível do personagem +1";
    } else if (rand > 69 && rand < 89) {
        trap += " Dano: "+rand1+" X Nível do personagem +2";
    } else if (rand > 88 && rand < 94) {
        trap += " Dano: Nível do personagem X 1,5 X "+rand1;
    } else {
        trap += " Dano: Nível do personagem X 2 X "+rand1;
    }
    return trap;
}
